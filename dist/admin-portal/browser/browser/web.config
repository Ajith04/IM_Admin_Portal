<configuration>
  <system.webServer>
    <staticContent>
      <mimeMap fileExtension=".json" mimeType="application/json" />
    </staticContent>
    <rewrite>
      <rules>
        <rule name="Angular Routes" stopProcessing="true">
          <match url="^(.*)$" />
          <action type="Rewrite" url="/browser/browser/{R:1}" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
